{
    "vars": {
        "id": "orchestrator",
        "services": [ "orchestrator" ],
        "default_targets": [ "start-orchestrator" ]
    },
    "args_for": "orchestrator",

    "mutate": [
        { "method": "load", "jspath": "/usecase/proto/root.json" },
        { "method": "union" },
        { "method": "load", "register": "d", "jspath": "/usecase/proto/defaults.json" },
        { "method": "union", "regpath": "vars", "srcregister": "d", "underlay": true },
        { "method": "load", "regpath": "orchestrator", "jspath": "/usecase/proto/orchestrator.json" },
        { "method": "expand" }
    ]
}
