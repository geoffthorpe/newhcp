{
    "setup": {
        "tag": "global",
        "exec": [
            "/hcp/python/hcp/tool/waitTouchfile.py",
            "/tpm_{machine}/tpm/ek.pub"
        ],
        "touchfile": "/tpm_{machine}/tpm/ek.pub"
    },
    "exec": "/hcp/swtpm.py",
    "healthcheck": [ "/hcp/swtpm.py", "--healthcheck" ],
    "state": "/tpm_{machine}",
    "sockdir": "/tpmsocket_{machine}",
    "tag": "services"
}
