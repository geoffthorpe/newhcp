{
    "priority": 50,
    "exec": "/hcp/python/hcp/svc/swtpm.py",
    "healthcheck": [ "/hcp/python/hcp/svc/swtpm.py", "--healthcheck" ],
    "state": "/tpm_{machine}",
    "sockdir": "/tpmsocket_{machine}",
    "toplevel": {
        "tpm-setup": {
            "priority": 10,
            "exec": [
                "/hcp/python/hcp/tool/waitTouchfile.py",
                "/tpm_{machine}/tpm/ek.pub"
            ],
            "block": "/tpm_{machine}/tpm/ek.pub",
	    "will_exit": true
        }
    }
}
