{
    "priority": 5,
    "exec": "/hcp/python/hcp/svc/swtpm.py",
    "healthcheck": [ "/hcp/python/hcp/svc/swtpm.py", "--healthcheck" ],
    "state": "/tpm_{id}",
    "sockdir": "/tpmsocket_{id}",
    "toplevel": {
        "setup_local": {
            "priority": 4,
            "exec": [ "mkdir", "-p", "/tpmsocket_{id}" ],
            "block": "/tpmsocket_{id}",
            "will_exit": true
        }
    }
}
