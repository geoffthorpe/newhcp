{
    "vars": {
        "id": "ahcp",
        "services": [ "attestsvc", "webapi" ],
        "default_targets": [ "setup-global", "setup-local", "start-services" ],
        "webapi_app": "/hcp/attestsvc/hcp_api.py",
        "webapi_fqdn": "ahcp.hcphacking.xyz",
        "webapi_port": 8080,
        "webapi_uwsgi_env": {
            "HOME": "/home/ahcpflask",
            "HCP_TRACEFILE": "/home/ahcpflask",
            "DIR": "/install-safeboot",
            "BINDIR": "/install-safeboot",
            "SAFEBOOT_DB_DIR": "/attestdb/db/current"
        },
        "webapi_uwsgi_uid": "ahcpflask"
    },

    "scope": [
        { "load": ".defaults", "path": "/usecase/proto_defaults.json" },
        { "vars": ".defaults" },
        { "import": ".", "source": "." },
        { "vars": ".vars" },
        { "loadunion": ".", "path": "/usecase/proto_root.json" },
        { "load": ".attestsvc", "path": "/usecase/proto_attestsvc_hcp.json" },
        { "load": ".webapi", "path": "/usecase/proto_webapi.json" },
        { "delete": ".webapi.https" }
    ]
}
