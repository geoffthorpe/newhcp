{
    "vars": {
        "id": "workstation1",
        "services": [ "attester", "do_nothing" ],
	"default_targets": [ "start-attester", "start-services" ]
    },

    "scope": [
        { "load": ".defaults", "path": "/usecase/proto_defaults.json" },
        { "vars": ".defaults" },
        { "import": ".", "source": "." },
        { "vars": ".vars" },
        { "load": ".", "path": "/usecase/proto_root.json" },
        { "load": ".attester", "path": "/usecase/proto_attester.json" },
        { "load": ".client", "path": "/usecase/proto_client.json" },
        { "load": ".do_nothing", "path": "/usecase/proto_do_nothing.json" },
        { "load": ".tpm", "path": "/usecase/proto_tpm_sidecar.json" }
    ]
}
